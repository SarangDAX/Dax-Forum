<h1> EDIT EXISTING POST : </h1>

<%= form_with(model: @post, local: true) do |f| %>

<div class="container">
    <p>
        <%= f.label :Title %><br>
        <%= f.text_field :title %>
    </p>

    <p>
        <%= f.label :Content %><br>
        <%= f.text_area :content %>
    </p>

    <p>
        <%= f.label :Channel %><br>
        <%= f.text_area :channel_id %>
    </p>

    <p>
        <%= f.submit %> 
    </p>
</div>

<% end %>